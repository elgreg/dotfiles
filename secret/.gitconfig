[alias]
	s = status -s
	l = log --left-right --graph --cherry-pick --oneline
	praise = blame
[user]
	name = Greg Lavallee
	email = greglavallee@wapolabs.com
[core]
	autocrlf = false
	excludesfile = /Users/elgreg/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
[help]
	autocorrect = 1
[color]
	ui = auto
